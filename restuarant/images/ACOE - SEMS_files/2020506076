  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-countto/1.2.0/jquery.countTo.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-countto/1.2.0/jquery.countTo.min.js.map"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/wow/1.1.2/wow.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-smooth-scroll/2.0.0/jquery.smooth-scroll.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery.bootstrapvalidator/0.5.3/js/bootstrapValidator.min.js"></script>
  <script type="text/javascript" src="https://acoe.annauniv.edu/sems/assests/js/home/main.js"></script>
  <!--<script src="https://acoe.annauniv.edu/sems/assests/js/bootstrapValidator.min.js"></script> -->
  <script src="https://acoe.annauniv.edu/sems/assests/js/sha512.js"></script>
  <script> 
    $(document).ready(function() {
      $('#login').bootstrapValidator({
        message: 'This value is not valid',
        feedbackIcons: {
          valid: 'glyphicon glyphicon-ok',
          invalid: 'glyphicon glyphicon-remove',
          validating: 'glyphicon glyphicon-refresh'
        },
        fields: {
          username: {
            validators: {
              notEmpty: {
                message: 'The Roll Number is required'
              },
              regexp: {
                regexp: /^([0-9])+$/,
                message: 'Roll Number must consists of digits 0-9 only'
              },
              stringLength: {
                min: 10,
                max: 11,
              }
            }
          },                
          password: {
            validators: {
              notEmpty: {
                message: 'The password is required'
              }
            }
          },
		  captcha_code: {
              validators: {
                notEmpty: {
                  message: 'The Captcha is required'
                }
              }
            }
        }
      })
      .on('success.form.bv', function(e) {
        var p = hex_sha512($('#password').val());
        $('#password').val(p);
      });
    });
	$('.refresh-captcha').click(function(e){
  		$('.captcha-image').attr("src","https://acoe.annauniv.edu/sems/Login/captcha/"+new Date().getTime());
	});
  </script>
